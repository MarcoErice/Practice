@inject Practice.Interfaces.iTimeProvider myTimeProvider
@{
    //    if (myTimeProvider.Now.Year >= 2018 && myTimeProvider.Now.Month >= 2)
    var yyyymmdd = myTimeProvider.Now.Year * 10000 +
        myTimeProvider.Now.Month * 100 +
        myTimeProvider.Now.Day;
    if(yyyymmdd >= 20180201)
    {
        Layout = "_Layout20180201";
    }
    else
    {
        Layout = "_Layout";
    }

}
